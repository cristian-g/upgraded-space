{% extends "base.html.twig" %}
{% block title %}Hello page{% endblock %}
{% block styles %}
    {{ parent() }}
    <!--<link rel="stylesheet" type="text/css" href="{{ base_url() }}/assets/css/example.css">-->
{% endblock %}
{% block scripts %}
    {{ parent() }}
    <!--<script src="example.js"></script>-->
{% endblock %}
{% block header %}
    {#{{ parent() }}#}
    exemple

{% endblock %}
{% block content %}
    <form action="/user" method="post">
        <label for="username">Username: <input type="text" name="username" placeholder="username"></label>
        <label for="email">Email: <input type="email" name="email" placeholder="email"></label>
        <label for="password">Password: <input type="password" name="password" placeholder="password"></label>
        <button type="submit" name="button">Enviar</button>
    </form>

    {% if error %}
        {{ error }}
    {% endif %}
    {% for message in messages %}
        {{ message }}
    {% endfor %}
{% endblock %}
{% block footer %}
    {{ parent() }}
    exemple
{% endblock %}